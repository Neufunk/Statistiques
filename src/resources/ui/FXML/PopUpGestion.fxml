   <HBox alignment="CENTER" prefHeight="100.0" prefWidth="3000.0" style="-fx-background-color: #424242;" GridPane.columnSpan="3" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER">
