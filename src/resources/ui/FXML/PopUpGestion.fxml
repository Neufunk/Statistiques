<GridPane alignment="CENTER" maxHeight="3000.0" maxWidth="3000.0" prefHeight="380.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SoinsInfirmiers.ControllerPopUpGestion">
