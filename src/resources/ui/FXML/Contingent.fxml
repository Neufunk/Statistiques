            <JFXButton buttonType="RAISED" focusTraversable="false" onAction="#onGenerateButtonClick" prefHeight="32.0" prefWidth="81.0" styleClass="orange-button" stylesheets="/ui/CSS/Buttons.css" text="Exécuter" textFill="WHITE" VBox.vgrow="ALWAYS">
