      <Label fx:id="yearLabel1" text="Label" visible="false">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <Label fx:id="yearLabel2" text="Label" visible="false">
         <font>
            <Font name="System Bold" size="15.0" />
