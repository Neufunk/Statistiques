               </VBox.margin>
            </JFXComboBox>
            <JFXComboBox fx:id="comboPeriode" focusColor="#96bb37" labelFloat="true" promptText="Période" stylesheets="@../CSS/ComboBox.css" unFocusColor="#00707e">
